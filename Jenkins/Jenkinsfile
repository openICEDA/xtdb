pipeline {
  agent any
  stages {
    stage('Hello') {
      steps {
        // Print "Hello World"
        echo 'Hello World'
        echo 'This is added'
      }
    }
    stage('Build') {
      steps {
        // Clean before build
        cleanWs()
        makedir build
        #sh 'cmake -Bbuild -H.'
        sh 'cmake --build build'
        sh 'cd build'
        sh 'make'
      }
    }
  }
}

