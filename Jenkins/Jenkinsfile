pipeline {
  agent any
  stages {
    stage('Hello') {
      steps {
        // Print "Hello World"
        echo 'Hello World in PR'
        echo 'This is added'
      }
    }
    stage('Build') {
      steps {
        sh 'mkdir build'
        sh 'cd build'
        sh 'cmake ..'
        sh 'make'
      }
    }
  }
}
